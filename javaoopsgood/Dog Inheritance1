
import java.io.*;
import java.util.*;
class Dog{
    private String name;
    Dog(String name){
        this.name=name;
    }
    public String getName(){
        return name;
    }    
}
class Greyhound extends Dog{
    private String color;
    private int weight;
    Greyhound(String name,String color,int weight){
        super(name);
        this.color=color;
        this.weight=weight;
    }
    public String getColor(){
        return color;}
    public int getWeight(){
        return weight;}
    
}
