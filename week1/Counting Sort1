   public static List<Integer> countingSort(List<Integer> arr) {
    // Write your code here
    
    ArrayList<Integer> valueCount = new ArrayList<>();
    for(int i = 0; i < 100; i++){
        valueCount.add(0);
        
    }
    for(int value : arr){
        valueCount.set(value, valueCount.get(value)+1);
    }
    
    return valueCount;
    }
}
 
