import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner input =new Scanner(System.in);
        String a=input.next();
        LinkedHashSet<Character>set=new LinkedHashSet<Character>();
        for(int i=0;i<a.length();i++){
            char c=a.charAt(i);     
            if(!set.contains(c))
                set.add(c);
        }
        for(Character c:set)
            System.out.print(c);
        }

            
    

    
