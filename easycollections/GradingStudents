
class Result {
    public static List<Integer> gradingStudents(List<Integer> grades) {
     List<Integer>grade=new ArrayList<>();
        for(Integer val:grades){
            if(val<38)
                grade.add(val);
            else if(val%5 <3)
                grade.add(val);
            else
                grade.add(val+(5-val%5));
            
    }
        return grade;

}
}
